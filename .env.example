# Sniper Bot Environment Variables Template
# Copy file ini ke .env dan isi dengan nilai yang sesuai

# Bybit API Configuration
BYBIT_API_KEY=your_bybit_api_key_here
BYBIT_SECRET=your_bybit_secret_here
BYBIT_TESTNET=False

# Webhook Configuration
AUTH_TOKEN=sniper-bybit-production-2024
WEBHOOK_PORT=5001

# Trading Configuration
TARGET_SYMBOL=ETHUSDT
POSITION_SIZE_PERCENT=1.0
MIN_BALANCE_USD=5.0

# Dashboard Configuration
DASHBOARD_PORT=8080
DASHBOARD_SECRET_KEY=your_secret_key_here

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=bybit_production.log

# Risk Management
MAX_DAILY_LOSS=100.0
EMERGENCY_STOP=False
COOLDOWN_MINUTES=5

# Confidence Settings
MIN_CONFIDENCE_PERCENT=30.0